<UserControl x:Class="MiniKartoteka.Modules.AddNewPatientModule.Views.AddNewPatientModuleButton"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MiniKartoteka.Modules.AddNewPatientModule.Views"
             xmlns:inf="clr-namespace:MiniKartoteka.Infrastructure.Concrete.Mvvm;assembly=MiniKartoteka.Infrastructure"
             xmlns:view="clr-namespace:MiniKartoteka.Modules.AddNewPatientModule.Views"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid Background="Azure">
        <Button Margin="5" Content="Navigate to Add Patient"
                Command="{x:Static inf:ApplicationCommands.NavigateCommand}"
                CommandParameter="{x:Type view:ContentView}"/>
    </Grid>
</UserControl>
